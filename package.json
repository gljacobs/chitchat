{
  "name": "chitchat",
  "version": "1.0.0",
  "description": "chat app (slack/discord clone)",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gljacobs/chitchat.git"
  },
  "author": "Gabriel Jacobs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gljacobs/chitchat/issues"
  },
  "homepage": "https://github.com/gljacobs/chitchat#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socketio-jwt": "^4.5.0",
    "uws": "^10.148.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
